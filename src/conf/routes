# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Users
POST    /1.1.0/users/create.json                           yokohama.yellow_man.sena.controllers.UsersController.postCreate()
POST    /1.1.0/users/login.json                            yokohama.yellow_man.sena.controllers.UsersController.postLogin()

# AccountStocks
GET     /1.1.0/account_stocks/list.json                    yokohama.yellow_man.sena.controllers.AccountStocksController.getList()
POST    /1.1.0/account_stocks/create.json                  yokohama.yellow_man.sena.controllers.AccountStocksController.postCreate()